{
  "name": "node-onesky-cli",
  "version": "1.0.1",
  "description": "CLI to synchronize translation file in OneSky app with developer's source code.",
  "main": "onesky.js",
  "engines": {
    "node": ">=6.10.0"
  },
  "scripts": {
    "test": "mocha --recursive ./test",
    "cov": "nyc --report-dir ./artifacts/coverage --reporter=html --reporter=text --reporter=text-summary --reporter=lcov _mocha  --recursive ./test",
    "lint": "eslint ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/made-aryadinata/node-onesky-cli.git"
  },
  "keywords": [
    "onesky",
    "translation",
    "node"
  ],
  "author": "Made Aryadinata",
  "license": "ISC",
  "bin": {
    "onesky": "./onesky.js"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "jsonfile": "^3.0.0",
    "onesky-utils": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-security": "^1.3.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0"
  }
}
