{
  "name": "node-onesky-cli",
  "version": "1.0.3",
  "description": "CLI to synchronize translation file in OneSky app with developer's source code.",
  "main": "onesky.js",
  "engines": {
    "node": ">=6.10.0"
  },
  "scripts": {
    "test": "mocha --recursive ./test",
    "cov": "nyc --report-dir ./artifacts/coverage --reporter=html --reporter=text --reporter=text-summary --reporter=lcov _mocha  --recursive ./test",
    "lint": "eslint ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/made-aryadinata/node-onesky-cli.git"
  },
  "keywords": [
    "onesky",
    "translation",
    "node"
  ],
  "author": "Made Aryadinata",
  "license": "ISC",
  "bin": {
    "onesky": "./onesky.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "jsonfile": "^5.0.0",
    "onesky-utils": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
